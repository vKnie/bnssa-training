{
  "expo": {
    "name": "bnssa-training",
    "slug": "bnssa-training",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo_app_512.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/logo_app_512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/logo_app_512.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/icons/logo_app_512.png"
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
